@{
    Layout = null;
}
@model PrisustvoNaNastaviPrikazVM
Ime studenta 
@Model.ImeStudenta


<h1>Prisustvo na nastavi</h1>

<table class="table table-bordered">
    <thead>
    <tr>
        <th>Predmet</th>
        <th>Datum</th>
        
    </tr>
    </thead>
    
    <tbody>
    @foreach (var x in Model.Zapisi)
    {
        <tr>
            <td>@x.Predmet</td>
            <td>@x.Datum</td>
       
          
        </tr>
    }
    </tbody>
</table>

